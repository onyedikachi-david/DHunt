{
    "compilerOptions": {
        "target": "es2020",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "paths": {
            "@utils": ["./utils/"],
            "@utils/*": ["./utils/*"],
            "@typechain": ["./typechain-types/"],
            "@typechain/*": ["./typechain-types/*"]
        }
    }
}
